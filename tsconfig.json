{
  "compilerOptions": {
      "target": "es5",
      "lib": [
          "dom",
          "dom.iterable",
          "esnext"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noImplicitAny": true,
      "noEmit": false,
      "jsx": "react-jsx",
      "baseUrl": ".",
  },
  "include": [
      "./apps/*",
      "./packages/*",
      "."
  ],
  "exclude": [
      "./apps/*/dist/*"
  ]
}